{
  "expo": {
    "name": "Guess The Word",
    "slug": "guess-the-word",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/logo.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/logo.png",
      "resizeMode": "contain",
      "backgroundColor": "#0b4c85",
      "showName": false
    },
    "ios": {
      "supportsTablet": true
    },
