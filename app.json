      "adaptiveIcon": {
        "foregroundImage": "./assets/logo.png",
        "backgroundColor": "#0b4c85"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.desielove.guesstheword"
    },
    "web": {
      "favicon": "./assets/logo.png"
    },
    "plugins": [
      "expo-font",
      "expo-sqlite"
    ],
    "extra": {
      "eas": {
        "projectId": "d95e00c7-12e3-4107-ab97-5de34c8fca48"
      }
    }
  }
}
